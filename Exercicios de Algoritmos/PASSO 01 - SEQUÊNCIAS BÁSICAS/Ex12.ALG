algoritmo "Ex11"
var
   preco,desconto: real
inicio
      Escreva("Digite o preço: ")
      leia(preco)
      desconto <- preco * 0.95
      Escreva("O valor",preco," com 5% de desconto fica",desconto)
fimalgoritmo

