algoritmo "Ex6"
var
   num,antecessor,sucessor: real
inicio
      Escreva("Digite um número: ")
      Leia(num)
      antecessor <- num - 1
      sucessor <- num + 1
      Escreval("O antecessor de",num," é",antecessor)
      Escreval("O sucessor de",num," é",sucessor)
fimalgoritmo

