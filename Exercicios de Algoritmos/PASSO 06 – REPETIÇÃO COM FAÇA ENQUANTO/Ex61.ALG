algoritmo "Ex61"
var
   contador: inteiro
inicio
   contador <- 0

   Enquanto contador <= 30 faca
      escreva(contador, " ")
      contador <- contador + 3
  FimEnquanto

   escreva("Acabou!")
fimalgoritmo

