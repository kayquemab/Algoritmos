algoritmo "Ex42"
var
   N, i: inteiro
inicio

      Escreva("Digite um valor: ")
      Leia(N)
      
      Para i de 1 até N passo 1 faça
           Escreva(i)
           Se 1 > N então
              Escreva(" ")
           FimSe
      FimPara
      
      Escreva(" Acabou!")
fimalgoritmo

