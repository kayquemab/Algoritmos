algoritmo "Ex39"
var
      i: inteiro
inicio
      Para i de 10 até 3 passo -1 faça
           Escreva(i)
           Se i > 3 então
           Escreva(" ")
           FimSe
      FimPara
      Escreva(" Acabou!")
fimalgoritmo

