algoritmo "Ex41"
var
   i: inteiro
inicio
      Para i de 100 até 0 passo -5 faça
           Escreva(i)
           
           Se i > 0 então
           Escreval(" ")
           FimSe
      FimPara
      
      Escreval(" Acabou!")
fimalgoritmo

